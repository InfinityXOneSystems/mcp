{
  "name": "ai-ecosystem-unified",
  "version": "1.0.0",
  "description": "Unified AI Agent Ecosystem with modular architecture and cross-platform support",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "modules": "node dist/modules/index.js",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "module-status": "node -e \"require('./dist/modules').getModuleRegistry().getAllStatuses().then(s => console.table(s))\"",
    "diagnostics": "node scripts/diagnostics.js",
    "docker:build": "docker build -t ai-ecosystem .",
    "docker:run": "docker run -p 8080:8080 --env-file .env ai-ecosystem",
    "docs": "typedoc src/**/*.ts"
  },
  "keywords": [
    "ai",
    "agents",
    "automation",
    "orchestration",
    "cross-platform",
    "modular",
    "typescript",
    "python",
    "monitoring",
    "sync"
  ],
  "author": "InfinityX One Systems",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/InfinityXOneSystems/AI.git"
  },
  "bugs": {
    "url": "https://github.com/InfinityXOneSystems/AI/issues"
  },
  "homepage": "https://github.com/InfinityXOneSystems/AI#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.0",
    "winston": "^3.11.0",
    "axios": "^1.6.0",
    "dockerode": "^4.0.2",
    "simple-git": "^3.20.0",
    "redis": "^4.6.0",
    "node-cron": "^3.0.3",
    "puppeteer": "^21.0.0",
    "uuid": "^9.0.0",
    "dotenv": "^16.3.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "joi": "^17.9.0",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/ws": "^8.18.1",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "@types/uuid": "^9.0.0",
    "@types/cors": "^2.8.0",
    "@types/compression": "^1.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "prettier": "^3.0.0",
    "typedoc": "^0.25.0",
    "nodemon": "^3.0.0",
    "supertest": "^6.3.0",
    "@types/supertest": "^2.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn",
      "@typescript-eslint/explicit-function-return-type": "off"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  }
}